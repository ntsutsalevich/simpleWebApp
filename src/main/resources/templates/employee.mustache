<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Employee</title>
    <style>
        table {
            border-collapse: collapse;
        }

        table, td, th {
            border: 1px solid #999;
            padding: 5px;
        }
    </style><head>
<body>
<table>
    <tr>
        <th>Employee ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Department ID</th>
        <th>Job title</th>
        <th>Gender</th>
        <th>Date Of Birthday</th>
    </tr>

    {{#employee}}
        <tr>
            <td>{{employeeId}}</td>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{departmentId}}</td>
            <td>{{jobTitle}}</td>
            <td>{{gender}}</td>
            <td>{{dateOfBirth}}</td>
        </tr>


    {{/employee}}
</table>

<button id="button" onclick="window.location='main'"> Back</button>
</body>
</html>